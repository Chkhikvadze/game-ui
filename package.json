    "eject": "react-scripts eject",
    "lint": "eslint --ext .ts,.tsx src",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
