    "build": "GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true react-app-rewired build",
    "build:dev": "GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true env-cmd -f .env.develop react-app-rewired build --scripts-version react-scripts",
